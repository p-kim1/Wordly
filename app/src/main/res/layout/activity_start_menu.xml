<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".startMenu">

    <TextView
        android:id="@+id/startMenu_TextView_fromWord"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/startMenu_TextView_toWord"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/startMenu_Button_newPuzzle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Puzzle"/>

    <Button
        android:id="@+id/startMenu_Button_play"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Play Game" />



</android.support.constraint.ConstraintLayout>